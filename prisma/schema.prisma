generator client {
  provider      = "prisma-client-js"
  // Generate native query engine for the platform where build runs (Nixpacks/native)
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  key       String   @id @default(uuid())
  url       String
  buildId   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
